{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore node_modules src/server.ts",
    "start": "env $(cat .env) ts-node src/server.ts",
    "test": "env $(cat .env.tests) jest --no-cache",
    "migrate:make": "knex --knexfile src/config/knexfile.ts migrate:make -x ts --",
    "migrate:latest": "knex --knexfile src/config/knexfile.ts migrate:latest",
    "migrate:rollback": "env $(cat .env) knex --knexfile src/config/knexfile.ts migrate:rollback",
    "db:populate": "ts-node src/populate.ts"
  },
  "dependencies": {
    "amqplib": "^0.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.13.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "indicative": "^7.4.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.16",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
